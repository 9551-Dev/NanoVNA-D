{
    "C_Cpp.errorSquiggles": "Disabled",
    "cpplint.filters": [
        "-build/include_subdir",
        "-build/include_order",
        "-readability/casting",
        "-whitespace/comments"
    ],
    "files.associations": {
        "*.json.liquid": "json",
        "*.yaml.liquid": "yaml",
        "*.md.liquid": "markdown",
        "*.js.liquid": "liquid-javascript",
        "*.css.liquid": "liquid-css",
        "*.scss.liquid": "liquid-scss",
        "cstdint": "c"
    }
}